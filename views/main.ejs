    <section class="banner_part">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-5 col-xl-5">
                    <div class="banner_text">
                        <div class="banner_text_iner">
                            <h5>We are here for your care</h5>
                            <h1>Emergency Ambulance Call</h1>

                            <a href="#" class="btn_2" id="quick">Find any ambulance</a><br>
                            <div id="closest_ambulance" style="visibility: hidden;">
                                <form method="POST" class="form-group">
                                    <input class="form-control" type="text" , placeholder="X_coordinate" name="x">
                                    <input class="form-control" type="text" , placeholder="Y_coordinate" name="y">
                                    <input class="btn btn-primary" type="submit" value="Submit Details">

                                </form>
                            </div>
                            <a href="#" class="btn_2" id="best">Find a special one</a>
                            <div id="ambulance_by_disease" style="visibility: hidden;">
                                <form method="POST" class="form-group">
                                    <input class="form-control" type="text" , placeholder="X_coordinate" name="x">
                                    <input class="form-control" type="text" , placeholder="Y_coordinate" name="y">
                                    <input class="form-control" type="text" , placeholder="Medical Problem"
                                        name="problem">
                                    <input class="btn btn-primary" type="submit" value="Submit Details">

                                </form>
                            </div>

                            <p>Find the quickest ambulance or one with special facilities as per your requirements.
                                Simple interface and easy to use </p>
                            <script>
                                document.getElementById('quick').addEventListener('click', (e) => {
                                    e.preventDefault();

                                    document.getElementById('closest_ambulance').style.visibility = "visible";
                                    document.getElementById('ambulance_by_disease').style.visibility = "hidden";
                                });
                                document.getElementById('best').addEventListener('click', (e) => {
                                    e.preventDefault();

                                    document.getElementById('ambulance_by_disease').style.visibility =
                                        "visible";
                                    document.getElementById('closest_ambulance').style.visibility = "hidden";
                                })
                            </script>
                        </div>
                    </div>
